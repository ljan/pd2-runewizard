# the game version of Runewizard : vanilla D2 LoD, or a mod
VITE_GAME_NAME='Project Diablo 2'

# version of the vanilla/modded Diablo II that Runewizard is compatible with
VITE_GAME_VERSION='Season 9'

# set 'false' to turn off the header, to eg. include within an existing layout
VITE_LAYOUT_HEADER=true

# main site link displayed in the header (leave empty to disable the link)
VITE_URL_MAIN_SITE=https://kvothed2.github.io/pd2-runewizard/

# link to the Github repository (origin, or fork) displayed in the header
VITE_URL_GITHUB_REPO=https://github.com/kvothed2/pd2-runewizard/
